{
  "name": "plain-text-data-to-json",
  "version": "1.0.0",
  "description": "Transform a simple plain-text database to JSON",
  "license": "MIT",
  "keywords": [
    "plain-text",
    "database",
    "json"
  ],
  "files": [
    "index.js"
  ],
  "repository": "https://github.com/wooorm/plain-text-data-to-json",
  "bugs": "https://github.com/wooorm/plain-text-data-to-json/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "dependencies": {
    "has": "^1.0.1",
    "trim": "0.0.1"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "cept": "^1.0.1",
    "esmangle": "^1.0.1",
    "nyc": "^10.0.0",
    "remark-cli": "^2.0.0",
    "remark-preset-wooorm": "^1.0.0",
    "tape": "^4.0.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --bare -s plain-text-data-to-json > plain-text-data-to-json.js",
    "build-mangle": "esmangle plain-text-data-to-json.js > plain-text-data-to-json.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "xo": {
    "space": true,
    "rules": {
      "unicorn/explicit-length-check": "off"
    },
    "ignores": [
      "plain-text-data-to-json.js"
    ]
  },
  "remarkConfig": {
    "output": true,
    "presets": "wooorm"
  }
}
